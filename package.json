{
  "name": "mcp-fraxlend",
  "version": "0.0.1",
  "description": "Mcp server for Fraxlend access",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm run build",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "packageManager": "pnpm@10.7.1",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.js"
    },
    "./tool-selector": {
      "import": "./src/tool-selector.ts",
      "require": "./src/tool-selector.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IQAIcom/mcp-fraxlend.git"
  },
  "keywords": [
    "mcp",
    "Fraxlend",
    "iqagents"
  ],
  "author": "IQAI",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IQAIcom/mcp-fraxlend/issues"
  },
  "devDependencies": {
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "homepage": "https://github.com/IQAIcom/mcp-fraxlend#readme",
  "dependencies": {
    "@types/node": "^22.15.19",
    "@biomejs/biome": "*",
    "dedent": "^1.6.0",
    "fastmcp": "^1.27.7",
    "gql.tada": "^1.8.10",
    "graphql-request": "^7.1.2",
    "viem": "^2.29.2",
    "zod": "^3.25.1"
  }
}
